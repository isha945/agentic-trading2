# Environment Variables
# Copy this file to .env and fill in the values

# WalletConnect Cloud project ID (required)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=

# Application name for wallet dialogs
NEXT_PUBLIC_APP_NAME=My DApp

# Pyth price feed ID (32-byte hex string) (required)
PYTH_PRICE_FEED_ID=0xff61491a931112ddf1bd8147cd1b641375f79f5825126d665480874634fd0ace

# Chain for Pyth oracle (e.g. ARBITRUM or ARBITRUM_SEPOLIA) (required)
PYTH_CHAIN=ARBITRUM

# Chainlink Data Feed contract address (AggregatorV3Interface) (required)
CHAINLINK_FEED_ADDRESS=0x639Fe6ab55C921f74e7fac1ee960C0B6293ba612

# Chain for Chainlink feed (e.g. ARBITRUM or ARBITRUM_SEPOLIA) (required)
CHAINLINK_CHAIN=ARBITRUM

# Maxxit API base URL
MAXXIT_API_BASE_URL=http://localhost:5000

# Network for Ostium trading (arbitrum or arbitrum-sepolia) (required)
NEXT_PUBLIC_OSTIUM_NETWORK=arbitrum